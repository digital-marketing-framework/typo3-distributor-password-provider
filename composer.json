{
    "name": "digital-marketing-framework/typo3-distributor-password-provider",
    "description": "Digital Marketing Framework - Distributor - Password",
    "license": "GPL-2.0+",
    "type": "typo3-cms-extension",
    "keywords": [
        "TYPO3",
        "extension",
        "form",
        "password"
    ],
    "authors": [
        {
            "name": "Mediatis AG",
            "email": "info@mediatis.de",
            "homepage": "https://www.mediatis.de",
            "role": "Developer"
        }
    ],
    "homepage": "https://www.mediatis.de",
    "require": {
        "php": "^8.1",
        "digital-marketing-framework/distributor-password-provider": "dev-feature/MFR-23-port-from-formrelay",
        "digital-marketing-framework/typo3-distributor-core": "dev-main",
        "typo3/cms-core": "^11.5 || ^12.4"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:digital-marketing-framework/distributor-password-provider.git",
            "only": [
                "digital-marketing-framework/distributor-password-provider"
            ]
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "DigitalMarketingFramework\\Typo3\\Distributor\\PasswordProvider\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DigitalMarketingFramework\\Typo3\\Distributor\\PasswordProvider\\Tests\\": "Tests"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "phpstan/extension-installer": true,
            "typo3/class-alias-loader": true,
            "typo3/cms-composer-installers": true
        },
        "bin-dir": ".Build/bin",
        "vendor-dir": ".Build/vendor"
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "digitalmarketingframework_distributor_password_provider",
            "web-dir": ".Build/Web"
        }
    }
}
